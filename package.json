{
  "name": "budget-client",
  "version": "0.1.0",
  "description": "Simple Budget Client App.",
  "main": "index.js",
  "scripts": {
    "dev": "node ./node_modules/webpack/bin/webpack.js --config webpack.config.js && webpack-dev-server --port 8010",
    "test": "node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "build": "node ./node_modules/webpack/bin/webpack.js --config webpack.production.config.js",
    "build:surge": "node ./node_modules/webpack/bin/webpack.js --config webpack.production.config.js && surge ./prod --domain andcounting.surge.sh"
  },
  "author": "Barric Reed & Mike Zimmerman",
  "license": "Apache 2.0",
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "0.23.1",
    "install": "^0.4.1",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "csv": "0.4.6",
    "d3": "4.0.0-alpha.2",
    "history": "^1.17.0",
    "immutable": "^3.7.5",
    "moment": "2.10.6",
    "moment-range": "2.1.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-d3": "0.4.0",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.5",
    "redux-thunk": "1.0.3",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": "4.2.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-strict-mode"
    ]
  }
}
